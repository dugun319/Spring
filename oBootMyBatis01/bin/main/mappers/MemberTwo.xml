<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.oracle.oBootMyBatis01.MemberTwoMapper">
 

	<select id="MemberCount"  	parameterType="java.lang.String" 	resultType="int">
		SELECT COUNT(*) 
			FROM member2
			WHERE id = #{id}	
	</select>
	
	<select id="ListMember"  	resultType="MemberTwo">
		SELECT *FROM member2
			ORDER BY id DESC	
	</select>
	
</mapper>